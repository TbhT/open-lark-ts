{
  "name": "lark-sdk",
  "version": "0.2.3",
  "description": "lark bot sdk",
  "main": "dist/index.js",
  "types": "types",
  "repository": "git@github.com:TbhT/lark-sdk.git",
  "author": "TbhT <hustapple132@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "pre-build": "tsc",
    "doc": "./node_modules/.bin/typedoc",
    "build": "npm run pre-build && npm run doc",
    "test": "./node_modules/.bin/jest tests/apis/Callback.test.ts -- --coverage && npm run test:badges",
    "test:badges": "./node_modules/.bin/jest-coverage-badges"
  },
  "files": [
    "dist",
    "types"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.1",
    "@types/moment": "^2.13.0",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "debug": "^4.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.4.0",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.7",
    "typedoc-plugin-external-module-name": "^4.0.3",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.8.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --color"
    ]
  },
  "dependencies": {
    "axios": "^0.19.2",
    "form-data": "^3.0.0",
    "moment": "^2.24.0"
  }
}
